<html class="no-js"> <!--<![endif]-->
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>{{ page['meta']['title'] }}</title>
	<meta name="description" content="">
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
	
	<!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
	
	<link rel="stylesheet" href="/ui/style.css">
	{% for row in page['css'] %}
		<link rel="stylesheet" href="{{ row }}">
	{% endfor %}
	{% if page['template_css'] %}
		<link rel="stylesheet" href="{{ page['template_css'] }}">
	{% endif %}
	
	
	<script src="/vendor/components/modernizr/modernizr.js"></script>
</head>
<body >

	{% include "_navbar.twig" ignore missing %}
	
	
	
	<section class="container" id="page-content">
		{% include page['template'] %}
	</section>
	
	{% include "_footer.twig" ignore missing  %}


<script src="/ui/javascript.js"></script>


{% for row in page['js'] %}
	<script src="{{ row }}"></script>
{% endfor %}
{% if page['template_js'] %}
	<script src="{{ page['template_js'] }}"></script>
{% endif %}










{% if page['template_jtmpl'] %}
	{% include page['template_jtmpl'] %}
{% endif %}

{% include 'template/template.jtmpl' %}
	<div class="modal fade" tabindex="-1" role="dialog" id="modal-popup" ></div>

</body>
</html>
